    <script>
        $(document).ready(function() {
            checkWidth();
            $( window ).resize(function() {
                checkWidth();
            });
        });
        function checkWidth() {
            if($(window).width() < 640) {
                $(".uk-subnav-pill").removeAttr("uk-switcher").hide();
                $(".uk-accordion-title").show();
                $(".uk-switcher").attr("uk-accordion", "true").removeClass("uk-switcher");
            } else {
                $(".uk-accordion-title").hide();
                $(".uk-accordion").addClass("uk-switcher").removeAttr("uk-accordion").removeClass("uk-accordion");
                $(".uk-accordion-content").removeAttr("hidden").removeAttr("aria-hidden");
                $(".uk-subnav-pill").attr("uk-switcher", "connect: .swicth-con").show();
            }
        }
    </script>
